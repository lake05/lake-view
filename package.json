{
  "name": "lake-view",
  "version": "0.0.0-beta.1",
  "title": "lakeView",
  "description": "A Component Library for Vue.js.",
  "homepage": "https://github.com/lake05/lake-view/blob/master/README.md",
  "keywords": [
    "tailwindcss",
    "components",
    "vue",
    "vue3",
    "vue components"
  ],
  "type": "module",
  "main": "dist/lake-view.umd.js",
  "module": "dist/lake-view.es.js",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lake-view.es.js",
      "require": "./dist/lake-view.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "packages"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lake05/lake-view.git"
  },
  "private": false,
  "author": "Lake",
  "license": "MIT",
  "bugs": {
    "type": "git",
    "url": "https://github.com/lake05/lake-view/issues"
  },
  "scripts": {
    "dev": "vite",
    "build:doc": "vue-tsc && vite build",
    "build": "vue-tsc && vite build --config vite.lib.config.ts",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run test && npm run build",
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx,.vue --fix"
  },
  "lint-staged": {
    "*{js,jsx,ts,tsx,vue}": "eslint --fix",
    "**/*.{js,jsx,json,scss,ts,tsx,vue}": "prettier --write"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-i18n": "^4.2.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.36.1"
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/test-utils": "^2.2.6",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.8.0",
    "happy-dom": "^8.1.1",
    "husky": "^8.0.2",
    "postcss": "^8.4.20",
    "postcss-import": "^15.1.0",
    "prettier": "2.8.1",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "unplugin-vue-define-options": "^1.1.1",
    "vite": "^4.0.0",
    "vite-plugin-css-injected-by-js": "^2.2.0",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.26.2",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.11"
  },
  "engines": {
    "node": ">= 16"
  }
}
